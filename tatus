[36m@@ -0,0 +1,90 @@[m
[32m+[m[32mfrom bs4 import BeautifulSoup[m
[32m+[m[32mfrom bs4.element import Tag[m
[32m+[m[32mimport networkx as nx[m
[32m+[m[32mimport matplotlib.pyplot as plt[m
[32m+[m
[32m+[m
[32m+[m[32mwith open("lecture_slides_code.html", "r", encoding='utf-8') as f:[m
[32m+[m[32m    html_content = f.read()[m
[32m+[m[32msoup = BeautifulSoup(html_content, 'html.parser')[m
[32m+[m
[32m+[m[32mrows = soup.find_all("tr")[m
[32m+[m
[32m+[m[32m# Extract lecture titles[m
[32m+[m[32mlecture_titles = [][m
[32m+[m
[32m+[m
[32m+[m[32mfor row in rows:[m
[32m+[m[32m    if isinstance(row, Tag):[m
[32m+[m[32m        cols = row.find_all("td")[m
[32m+[m[32m        if len(cols) >= 2:[m
[32m+[m[32m            title = cols[1].get_text(strip=True)[m
[32m+[m[32m            if title:  # Only consider non-empty titles[m
[32m+[m[32m                lecture_titles.append(title)[m
[32m+[m
[32m+[m[32m# Save the lecture titles to a text file[m
[32m+[m[32mwith open("lecture_titles.txt", "w", encoding='utf-8') as f:[m
[32m+[m[32m    for title in lecture_titles:[m
[32m+[m[32m        f.write(title + "\n")[m
[32m+[m
[32m+[m[32mprint(f"Extracted {len(lecture_titles)} lecture titles and saved to lecture_titles.txt")[m
[32m+[m
[32m+[m
[32m+[m[32m'''[m
[32m+[m[32mG = nx.DiGraph()[m
[32m+[m
[32m+[m[32m# Added nodes and linear edges[m
[32m+[m[32mfor i in range(len(lecture_titles)):[m
[32m+[m[32m    G.add_node(lecture_titles[i])[m
[32m+[m[32m    if i > 0:[m
[32m+[m[32m        G.add_edge(lecture_titles[i - 1], lecture_titles[i])[m
[32m+[m
[32m+[m[32m# TO visualize the graph[m
[32m+[m[32mplt.figure(figsize=(16, 8))[m
[32m+[m[32mpos = nx.spring_layout(G, k=0.8, seed=42)[m
[32m+[m[32mnx.draw(G, pos, with_labels=True, node_color='lightgreen', edge_color='gray',node_size=5000, font_size=4, font_weight='bold', arrows=True)[m
[32m+[m
[32m+[m[32mplt.title("MIT 6.0001 Lecture Prerequisite Graph", fontsize=12)[m
[32m+[m[32mplt.axis('off')[m
[32m+[m[32mplt.tight_layout()[m
[32m+[m[32mplt.show()[m
[32m+[m[32m'''[m
[32m+[m
[32m+[m[32mG = nx.DiGraph()[m
[32m+[m
[32m+[m[32m# Added nodes and linear edges[m
[32m+[m[32mfor i in range(len(lecture_titles)):[m
[32m+[m[32m    G.add_node(lecture_titles[i])[m
[32m+[m[32m    if i > 0:[m
[32m+[m[32m        G.add_edge(lecture_titles[i - 1], lecture_titles[i])[m
[32m+[m
[32m+[m[32m# TO visualize the graph[m
[32m+[m[32mplt.figure(figsize=(20, 12))  # Increased figure size[m
[32m+[m
[32m+[m[32m# Create a hierarchical layout with more vertical spacing[m
[32m+[m[32mpos = nx.kamada_kawai_layout(G)[m
[32m+[m[32mpos_dict = {}[m
[32m+[m[32mimport random[m
[32m+[m[32mfor i, node in enumerate(G.nodes()):[m
[32m+[m[32m    x, y = pos[node][m
[32m+[m[32m    pos_dict[node] = (x, y + i * 0.1)  # Add increasing vertical displacement[m
[32m+[m
[32m+[m[32m# Draw with adjusted parameters[m
[32m+[m[32mnx.draw(G, pos_dict,[m[41m [m
[32m+[m[32m        with_labels=True,[m[41m [m
[32m+[m[32m        node_color='lightgreen',[m[41m [m
[32m+[m[32m        edge_color='gray',[m
[32m+[m[32m        node_size=2000,[m[41m [m
[32m+[m[32m        font_size=8,  # Increased font size[m
[32m+[m[32m        font_weight='bold',[m[41m [m
[32m+[m[32m        arrows=True,[m
[32m+[m[32m        width=2,  # Thicker edges[m
[32m+[m[32m        arrowsize=10,  # Larger arrows[m
[32m+[m[32m        node_shape='o',  # Circular nodes[m
[32m+[m[32m        alpha=0.9)  # Slight transparency[m
[32m+[m
[32m+[m[32mplt.title("MIT 6.0001 Lecture Prerequisite Graph", fontsize=14, pad=20)[m
[32m+[m[32mplt.axis('off')[m
[32m+[m[32mplt.margins(0.2)  # Add margins around the graph[m
[32m+[m[32mplt.tight_layout()[m
[32m+[m[32mplt.show()[m
\ No newline at end of file[m
