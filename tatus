[1m+++ b/cg_visualization.py[m
[36m@@ -0,0 +1,56 @@[m
[32m+[m[32mfrom pyvis.network import Network[m
[32m+[m[32mimport networkx as nx[m
[32m+[m[32mimport json[m
[32m+[m[32mfrom networkx.readwrite import json_graph[m
[32m+[m
[32m+[m[32mwith open("concept_graph_full.json", "r", encoding="utf-8") as f:[m
[32m+[m[32m    data = json.load(f)[m
[32m+[m[32mG_loaded = json_graph.node_link_graph(data)[m
[32m+[m
[32m+[m[32m# Created a pyvis network[m
[32m+[m[32mnet = Network([m
[32m+[m[32m    height="800px",[m
[32m+[m[32m    width="100%",[m
[32m+[m[32m    bgcolor="#ffffff",[m
[32m+[m[32m    font_color=True,[m
[32m+[m[32m    directed=True[m
[32m+[m[32m)[m
[32m+[m
[32m+[m[32m# Converted from networkx[m
[32m+[m[32mnet.from_nx(G_loaded)[m
[32m+[m[32m#node properties[m
[32m+[m[32mfor node in net.nodes:[m
[32m+[m[32m    node["size"] = 20[m
[32m+[m[32m    node["color"] = "#97C2FC"[m
[32m+[m[32m    node["borderWidth"] = 2[m
[32m+[m[32m    node["borderColor"] = "#2B7CE9"[m
[32m+[m[32m    node["font"] = {"size": 12}[m
[32m+[m
[32m+[m[32m# edge properties[m
[32m+[m[32mfor edge in net.edges:[m
[32m+[m[32m    edge["color"] = "#848484"[m
[32m+[m[32m    edge["smooth"] = {"type": "continuous"}[m
[32m+[m[32m    edge["arrows"] = "to"[m
[32m+[m[32m    edge["arrows"] = {"to": {"enabled": True, "scaleFactor": 0.5}}[m
[32m+[m
[32m+[m[32m# Configure dynamics[m
[32m+[m[32mnet.set_options("""[m
[32m+[m[32m    var options = {[m
[32m+[m[32m        "physics": {[m
[32m+[m[32m            "enabled": true,[m
[32m+[m[32m            "stabilization": {"iterations": 100},[m
[32m+[m[32m            "barnesHut": {[m
[32m+[m[32m                "gravitationalConstant": -2000,[m
[32m+[m[32m                "springConstant": 0.04,[m
[32m+[m[32m                "springLength": 95[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m""")[m
[32m+[m
[32m+[m[32m# Saved the graph[m
[32m+[m[32mtry:[m
[32m+[m[32m    net.write_html("concept_graph.html")[m
[32m+[m[32m    print("Graph has been saved as 'concept_graph.html'. Please open it in a web browser to view.")[m
[32m+[m[32mexcept Exception as e:[m
[32m+[m[32m    print(f"Error saving graph: {str(e)}")[m
[1mdiff --git a/concept-graph.py b/concept-graph.py[m
[1mnew file mode 100644[m
[1mindex 0000000..30ca7d1[m
[1m--- /dev/null[m
[1m+++ b/concept-graph.py[m
[36m@@ -0,0 +1,64 @@[m
[32m+[m[32mimport networkx as nx[m
[32m+[m[32mimport matplotlib.pyplot as plt[m
[32m+[m[32mimport json[m
[32m+[m[32mfrom networkx.readwrite import json_graph[m
[32m+[m[32mconcept_map = {[m
[32m+[m[32m    "What is computation?": ["Computation", "Algorithms", "Problem Solving"],[m
[32m+[m[32m    "Branching and Iteration": ["Conditionals", "Loops", "Control Flow"],[m
[32m+[m[32m    "String Manipulation, Guess and Check, Approximations, Bisection": [[m
[32m+[m[32m        "String Manipulation", "Guess and Check", "Approximation", "Bisection Search"[m
[32m+[m[32m    ],[m
[32m+[m[32m    "Decomposition, Abstractions, Functions": ["Decomposition", "Abstraction", "Functions"],[m
[32m+[m[32m    "Tuples, Lists, Aliasing, Mutability, Cloning": [[m
[32m+[m[32m        "Tuples", "Lists", "Aliasing", "Mutability", "Cloning"[m
[32m+[m[32m    ],[m
[32m+[m[32m    "Recursion, Dictionaries": ["Recursion", "Dictionaries"],[m
[32m+[m[32m    "Testing, Debugging, Exceptions, Assertions": [[m
[32m+[m[32m        "Testing", "Debugging", "Exceptions", "Assertions"[m
[32m+[m[32m    ],[m
[32m+[m[32m    "Object Oriented Programming": ["Classes", "OOP", "Encapsulation"],[m
[32m+[m[32m    "Python Classes and Inheritance": ["Inheritance", "Subclasses", "Class Hierarchies"],[m
[32m+[m[32m    "Understanding Program Efficiency, Part 1": ["Big-O Notation", "Efficiency"],[m
[32m+[m[32m    "Understanding Program Efficiency, Part 2": ["Efficiency", "Space Complexity"],[m
[32m+[m[32m    "Searching and Sorting": ["Search Algorithms", "Sorting Algorithms"],[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mG = nx.DiGraph()[m
[32m+[m
[32m+[m[32m# Converted concept_map to a list of concept lists in lecture order[m
[32m+[m[32mconcept_lists = list(concept_map.values())[m
[32m+[m
[32m+[m[32m#  graph[m
[32m+[m[32mfor i, concepts in enumerate(concept_lists):[m
[32m+[m[32m    # Connected all concepts within the same lecture in sequence[m
[32m+[m[32m    for j in range(len(concepts) - 1):[m
[32m+[m[32m        G.add_edge(concepts[j], concepts[j + 1])[m
[32m+[m
[32m+[m[32m    # Connected last concept of this lecture to all concepts in the next lecture[m
[32m+[m[32m    if i < len(concept_lists) - 1:[m
[32m+[m[32m        last_concept = concepts[-1][m
[32m+[m[32m        next_concepts = concept_lists[i + 1][m
[32m+[m[32m        for next_concept in next_concepts:[m
[32m+[m[32m            G.add_edge(last_concept, next_concept)[m
[32m+[m
[32m+[m
[32m+[m[32mdata = json_graph.node_link_data(G)[m
[32m+[m
[32m+[m[32m# Saved to json file[m
[32m+[m[32mwith open("concept_graph_full.json", "w", encoding="utf-8") as f:[m
[32m+[m[32m    json.dump(data, f, indent=4)[m
[32m+[m
[32m+[m[32mprint("Full graph saved to concept_graph_full.json")[m
[32m+[m
[32m+[m
[32m+[m[32m'''[m
[32m+[m[32mplt.figure(figsize=(22, 14))[m
[32m+[m[32mpos = nx.spring_layout(G, k=0.5, seed=42)[m
[32m+[m[32mnx.draw(G, pos, with_labels=True,[m
[32m+[m[32m        node_color="lightblue", node_size=2500,[m
[32m+[m[32m        edge_color="gray", arrows=True, width=2, font_size=10)[m
[32m+[m[32mplt.title("Concept Graph from Manually Curated Lecture Concepts", fontsize=14)[m
[32m+[m[32mplt.axis("off")[m
[32m+[m[32mplt.tight_layout()[m
[32m+[m[32mplt.show()[m
