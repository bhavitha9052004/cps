[32m+[m[32m// src/app.ts[m
[32m+[m
[32m+[m[32mimport express from "express";[m
[32m+[m[32mimport bodyParser from "body-parser";[m
[32m+[m[32mimport learningPathRoutes from "./routes/learningPath";[m
[32m+[m[32mimport cors from 'cors';[m
[32m+[m[32m// frontend port[m
[32m+[m
[32m+[m
[32m+[m[32mconst app = express();[m
[32m+[m[32mconst PORT = 3000;[m
[32m+[m
[32m+[m[32mapp.use(bodyParser.json());[m
[32m+[m[32mapp.use("/api", learningPathRoutes);[m
[32m+[m
[32m+[m[32mapp.listen(PORT, () => {[m
[32m+[m[32m  console.log(`Server running on http://localhost:${PORT}`);[m
[32m+[m[32m});[m
[32m+[m[32mapp.use(cors({ origin: 'http://localhost:5173' }));[m[41m [m
