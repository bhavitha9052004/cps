[32m+[m[32m  <script>[m
[32m+[m[32mfunction hideLinksOffline(){[m
[32m+[m[32m  if (!window.navigator.onLine) {[m
[32m+[m[32m    const stripLinks = document.querySelectorAll(".strip-link-offline")[m
[32m+[m[32m    const hideOffline = document.querySelectorAll(".hide-offline")[m
[32m+[m[32m    const showOffline = document.querySelectorAll(".show-offline")[m
[32m+[m[32m    Array.prototype.forEach.call(stripLinks, function(link) {[m
[32m+[m[32m      link.outerHTML = link.innerHTML[m
[32m+[m[32m    })[m
[32m+[m[32m    Array.prototype.forEach.call(hideOffline, function(link) {[m
[32m+[m[32m      link.classList.add("d-none")[m
[32m+[m[32m    })[m
[32m+[m[32m    Array.prototype.forEach.call(showOffline, function(link) {[m
[32m+[m[32m      link.classList.remove("d-none")[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32mhideLinksOffline()[m
[32m+[m[32m</script>[m
[32m+[m
