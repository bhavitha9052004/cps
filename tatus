[1mdiff --git a/recommendation_engine.py b/recommendation_engine.py[m
[1mnew file mode 100644[m
[1mindex 0000000..64adbc9[m
[1m--- /dev/null[m
[1m+++ b/recommendation_engine.py[m
[36m@@ -0,0 +1,84 @@[m
[32m+[m[32mimport json[m
[32m+[m[32mimport heapq[m
[32m+[m
[32m+[m
[32m+[m[32mdef load_graph(graph_path):[m
[32m+[m[32m    with open(graph_path, 'r') as f:[m
[32m+[m[32m        graph_data = json.load(f)[m
[32m+[m
[32m+[m[32m    graph = {}[m
[32m+[m[32m    for link in graph_data["links"]:[m
[32m+[m[32m        src = link["source"][m
[32m+[m[32m        tgt = link["target"][m
[32m+[m[32m        if src not in graph:[m
[32m+[m[32m            graph[src] = [][m
[32m+[m[32m        graph[src].append(tgt)[m
[32m+[m[32m    return graph[m
[32m+[m
[32m+[m
[32m+[m[32mdef load_learner_profile(profile_path, learner_id="L001"):[m
[32m+[m[32m    with open(profile_path, 'r') as f:[m
[32m+[m[32m        profiles = json.load(f)[m
[32m+[m
[32m+[m[32m    for profile in profiles:[m
[32m+[m[32m        if profile["learner_id"] == learner_id:[m
[32m+[m[32m            return profile[m
[32m+[m[32m    raise ValueError(f"Learner ID '{learner_id}' not found in profile.")[m
[32m+[m
[32m+[m
[32m+[m[32mdef dijkstra_recommendation(graph, start_concepts, weak_concepts, target):[m
[32m+[m[32m    visited = set()[m
[32m+[m[32m    heap = [][m
[32m+[m
[32m+[m[32m    for concept in start_concepts:[m
[32m+[m[32m        heapq.heappush(heap, (0, [concept]))  # (cost, path)[m
[32m+[m
[32m+[m[32m    while heap:[m
[32m+[m[32m        cost, path = heapq.heappop(heap)[m
[32m+[m[32m        current = path[-1][m
[32m+[m
[32m+[m[32m        if current == target:[m
[32m+[m[32m            return {"path": path, "cost": cost}[m
[32m+[m
[32m+[m[32m        if current in visited:[m
[32m+[m[32m            continue[m
[32m+[m
[32m+[m[32m        visited.add(current)[m
[32m+[m
[32m+[m[32m        for neighbor in graph.get(current, []):[m
[32m+[m[32m            edge_cost = 5 if neighbor in weak_concepts else 1[m
[32m+[m[32m            heapq.heappush(heap, (cost + edge_cost, path + [neighbor]))[m
[32m+[m
[32m+[m[32m    return {"path": [], "cost": float('inf')}[m
[32m+[m
[32m+[m
[32m+[m[32mif __name__ == "__main__":[m
[32m+[m[32m    concept_graph_path = "concept_graph_full.json"[m
[32m+[m[32m    learner_profile_path = "learner_profiles.json"[m
[32m+[m[32m    target_concept = "Sorting"  # ✅ Updated to valid node[m
[32m+[m
[32m+[m[32m    # Load graph[m
[32m+[m[32m    graph = load_graph(concept_graph_path)[m
[32m+[m
[32m+[m[32m    # Check target exists[m
[32m+[m[32m    all_nodes = set(graph.keys()) | {n for neighbors in graph.values() for n in neighbors}[m
[32m+[m[32m    if target_concept not in all_nodes:[m
[32m+[m[32m        print(f"❌ Target concept '{target_concept}' not found in graph.")[m
[32m+[m[32m        exit()[m
[32m+[m
[32m+[m[32m    # Load learner profile[m
[32m+[m[32m    try:[m
[32m+[m[32m        learner = load_learner_profile(learner_profile_path)[m
[32m+[m[32m    except ValueError as e:[m
[32m+[m[32m        print(f"❌ {e}")[m
[32m+[m[32m        exit()[m
[32m+[m
[32m+[m[32m    result = dijkstra_recommendation([m
[32m+[m[32m        graph,[m
[32m+[m[32m        start_concepts=learner["mastered_concepts"],[m
[32m+[m[32m        weak_concepts=learner["weak_concepts"],[m
[32m+[m[32m        target=target_concept[m
[32m+[m[32m    )[m
[32m+[m
[32m+[m[32m    print("✅ Recommended Path:", result["path"])[m
[32m+[m[32m    print("💰 Total Cost:", result["cost"])[m
[1mdiff --git a/recommendations.json b/recommendations.json[m
[1mnew file mode 100644[m
[1mindex 0000000..a52ec92[m
[1m--- /dev/null[m
[1m+++ b/recommendations.json[m
[36m@@ -0,0 +1,10 @@[m
[32m+[m[32m[[m
[32m+[m[32m  {[m
[32m+[m[32m    "learner_id": "L001",[m
[32m+[m[32m    "recommended_path": ["Problem Solving", "Conditionals", "Loops", "Control Flow"][m
[32m+[m[32m  },[m
[32m+[m[32m  {[m
[32m+[m[32m    "learner_id": "L002",[m
[32m+[m[32m    "recommended_path": ["Algorithms", "Problem Solving", "Conditionals"][m
[32m+[m[32m  }[m
[32m+[m[32m][m
[1mdiff --git a/server.js b/server.js[m
[1mnew file mode 100644[m
[1mindex 0000000..62f8699[m
[1m--- /dev/null[m
[1m+++ b/server.js[m
[36m@@ -0,0 +1,14 @@[m
[32m+[m[32mconst express = require('express');[m
[32m+[m[32mconst fs = require('fs');[m
[32m+[m[32mconst cors = require('cors');[m
[32m+[m[32mconst app = express();[m
[32m+[m[32mconst PORT = 5000;[m
[32m+[m
[32m+[m[32mapp.use(cors());[m
[32m+[m
[32m+[m[32mapp.get('/api/lectures', (req, res) => {[m
[32m+[m[32m  const data = fs.readFileSync('lectures.json', 'utf-8');[m
[32m+[m[32m  res.json(JSON.parse(data));[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mapp.listen(PORT, () => console.log(`Server running on http://localhost:${PORT}`));[m
[1mdiff --git a/server.py b/server.py[m
[1mnew file mode 100644[m
[1mindex 0000000..d7b59be[m
[1m--- /dev/null[m
[1m+++ b/server.py[m
[36m@@ -0,0 +1,13 @@[m
[32m+[m[32mfrom flask import Flask, request, jsonify[m
[32m+[m[32mfrom recommendation_engine import recommend_for_learner[m
[32m+[m
[32m+[m[32mapp = Flask(__name__)[m
[32m+[m
[32m+[m[32m@app.route('/recommend', methods=['POST'])[m
[32m+[m[32mdef recommend():[m
[32m+[m[32m    data = request.get_json()[m
[32m+[m[32m    response = recommend_for_learner(data)[m
[32m+[m[32m    return jsonify(response)[m
[32m+[m
[32m+[m[32mif __name__ == '__main__':[m
[32m+[m[32m    app.run(debug=True)[m
[1mdiff --git a/server/.gitkeep b/server/.gitkeep[m
