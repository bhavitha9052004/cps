[32m+[m[32mimport requests[m
[32m+[m[32mimport time[m
[32m+[m[32mfrom bs4 import BeautifulSoup[m
[32m+[m[32mfrom bs4.element import Tag[m
[32m+[m[32mimport json[m
[32m+[m
[32m+[m[32murl = "https://ocw.mit.edu/courses/6-0001-introduction-to-computer-science-and-programming-in-python-fall-2016/pages/lecture-slides-code/"[m
[32m+[m
[32m+[m[32msession = requests.Session()[m
[32m+[m[32mtime.sleep(2)  # Sleep to avoid overwhelming the server[m
[32m+[m[32mr = session.get(url)[m
[32m+[m
[32m+[m[32mwith open("lecture_slides_code.html", "w", encoding='utf-8') as f:[m
[32m+[m[32m    f.write(r.text)[m
[32m+[m[32mprint("HTML content retrieved and saved to lecture_slides_code.html")[m
[32m+[m
[32m+[m[32mwith open("lecture_slides_code.html", "r", encoding='utf-8') as f:[m
[32m+[m[32m    html_content = f.read()[m
[32m+[m
[32m+[m[32msoup = BeautifulSoup(html_content, 'html.parser')[m
[32m+[m[32mrows = soup.find_all("tr")[m
[32m+[m
[32m+[m[32mlecture_titles = [][m
[32m+[m
[32m+[m[32mfor row in rows:[m
[32m+[m[32m    if isinstance(row, Tag):[m
[32m+[m[32m        cols = row.find_all("td")[m
[32m+[m[32m        if len(cols) >= 2:[m
[32m+[m[32m            title = cols[1].get_text(strip=True)[m
[32m+[m[32m            if title:[m
[32m+[m[32m                lecture_titles.append(title)[m
[32m+[m
[32m+[m[32m# Save lecture titles to a text file (optional)[m
[32m+[m[32mwith open("lecture_titles.txt", "w", encoding='utf-8') as f:[m
[32m+[m[32m    for title in lecture_titles:[m
[32m+[m[32m        f.write(title + "\n")[m
[32m+[m
[32m+[m[32mprint(f"Extracted {len(lecture_titles)} lecture titles and saved to lecture_titles.txt")[m
[32m+[m
[32m+[m[32m# Save lecture titles to JSON file[m
[32m+[m[32mwith open("lecture_titles.json", "w", encoding='utf-8') as f_json:[m
[32m+[m[32m    json.dump(lecture_titles, f_json, ensure_ascii=False, indent=2)[m
[32m+[m
[32m+[m[32mprint(f"Saved {len(lecture_titles)} lecture titles to lecture_titles.json")[m
[1mdiff --git a/generate_learner_profiles.py b/generate_learner_profiles.py[m
[1mnew file mode 100644[m
[1mindex 0000000..46727fa[m
[1m--- /dev/null[m
[1m+++ b/generate_learner_profiles.py[m
[36m@@ -0,0 +1,37 @@[m
[32m+[m[32mimport json[m
[32m+[m[32mimport random[m
[32m+[m
[32m+[m[32mconcepts = [[m
[32m+[m[32m    "Variables", "Data Types", "Operators", "Control Flow", "Loops",[m
[32m+[m[32m    "Functions", "Recursion", "Arrays", "Linked Lists", "Stacks", "Queues",[m
[32m+[m[32m    "Trees", "Binary Trees", "Binary Search Trees", "Heaps", "Hash Tables",[m
[32m+[m[32m    "Graphs", "Graph Traversal", "DFS", "BFS", "Sorting", "Merge Sort",[m
[32m+[m[32m    "Quick Sort", "Searching", "Binary Search", "Greedy Algorithms",[m
[32m+[m[32m    "Dynamic Programming", "Backtracking", "Memoization",[m
[32m+[m[32m    "Complexity Analysis", "Big O Notation", "Divide and Conquer",[m
[32m+[m[32m    "Bit Manipulation"[m
[32m+[m[32m][m
[32m+[m
[32m+[m[32mdef generate_profiles(num_profiles=50):[m
[32m+[m[32m    profiles = [][m
[32m+[m[32m    for i in range(1, num_profiles + 1):[m
[32m+[m[32m        learner_id = f"L{i:03}"[m
[32m+[m[32m        num_mastered = random.randint(4, 8)[m
[32m+[m[32m        num_weak = random.randint(2, 5)[m
[32m+[m
[32m+[m[32m        mastered = set(random.sample(concepts, num_mastered))[m
[32m+[m[32m        weak = set(random.sample([c for c in concepts if c not in mastered], num_weak))[m
[32m+[m
[32m+[m[32m        profile = {[m
[32m+[m[32m            "learner_id": learner_id,[m
[32m+[m[32m            "mastered_concepts": sorted(list(mastered)),[m
[32m+[m[32m            "weak_concepts": sorted(list(weak))[m
[32m+[m[32m        }[m
[32m+[m[32m        profiles.append(profile)[m
[32m+[m[32m    return profiles[m
[32m+[m
[32m+[m[32mif __name__ == "__main__":[m
[32m+[m[32m    learner_profiles = generate_profiles(50)[m
[32m+[m[32m    with open("learner_profiles.json", "w") as f:[m
[32m+[m[32m        json.dump(learner_profiles, f, indent=4)[m
[32m+[m[32m    print("✅ learner_profiles.json successfully created with 50 profiles.")[m
