[1mdiff --git a/server/src/routes/__tests__/learningPath.test.ts b/server/src/routes/__tests__/learningPath.test.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..e69de29[m
[1mdiff --git a/server/src/routes/learningPath.ts b/server/src/routes/learningPath.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..489eb00[m
[1m--- /dev/null[m
[1m+++ b/server/src/routes/learningPath.ts[m
[36m@@ -0,0 +1,26 @@[m
[32m+[m[32m// src/routes/learningPath.ts[m
[32m+[m[32mimport express,{Request, Response} from "express";[m
[32m+[m[32mimport { findShortestPath } from "../utils/shortestPath";[m
[32m+[m
[32m+[m[32mconst router = express.Router();[m
[32m+[m
[32m+[m[32mrouter.post("/learning-path", (req: Request, res: Response): void => {[m
[32m+[m[32m  const { start, end } = req.body;[m
[32m+[m
[32m+[m[32m  if (!start || !end) {[m
[32m+[m[32m    res.status(400).json({ error: "Missing start or end concept" });[m
[32m+[m[32m    return;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  const path = findShortestPath(start, end);[m
[32m+[m
[32m+[m[32m  if (!path) {[m
[32m+[m[32m    res.status(404).json({ error: "No path found between concepts" });[m
[32m+[m[32m    return;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  res.json({ path });[m
[32m+[m[32m  return;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mexport default router;[m
[1mdiff --git a/server/src/utils/shortestPath.ts b/server/src/utils/shortestPath.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..fd1736b[m
[1m--- /dev/null[m
[1m+++ b/server/src/utils/shortestPath.ts[m
[36m@@ -0,0 +1,59 @@[m
[32m+[m[32m// src/utils/shortestPath.ts[m
[32m+[m
[32m+[m[32mimport fs from "fs";[m
[32m+[m[32mimport path from "path";[m
[32m+[m
[32m+[m[32minterface Node {[m
[32m+[m[32m  id: string;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32minterface Link {[m
[32m+[m[32m  source: string;[m
[32m+[m[32m  target: string;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32minterface GraphJSON {[m
[32m+[m[32m  nodes: Node[];[m
[32m+[m[32m  links: Link[];[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//BFS for shortest path[m
[32m+[m[32mexport function findShortestPath([m
[32m+[m[32m  start: string,[m
[32m+[m[32m  end: string[m
[32m+[m[32m): string[] | null {[m
[32m+[m[32m  const dataPath = path.join(__dirname, "../data/concept_graph_full.json");[m
[32m+[m[32m  const raw = fs.readFileSync(dataPath, "utf-8");[m
[32m+[m[32m  const graphData: GraphJSON = JSON.parse(raw);[m
[32m+[m
[32m+[m[32m  const adjList: Record<string, string[]> = {};[m
[32m+[m
[32m+[m[32m  // Built adjacency list[m
[32m+[m[32m  for (const node of graphData.nodes) {[m
[32m+[m[32m    adjList[node.id] = [];[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  for (const link of graphData.links) {[m
[32m+[m[32m    adjList[link.source].push(link.target);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // BFS[m
[32m+[m[32m  const queue: string[][] = [[start]];[m
[32m+[m[32m  const visited = new Set<string>();[m
[32m+[m
[32m+[m[32m  while (queue.length > 0) {[m
[32m+[m[32m    const path = queue.shift()!;[m
[32m+[m[32m    const node = path[path.length - 1];[m
[32m+[m
[32m+[m[32m    if (node === end) return path;[m
[32m+[m[32m    if (visited.has(node)) continue;[m
[32m+[m
[32m+[m[32m    visited.add(node);[m
[32m+[m
[32m+[m[32m    for (const neighbor of adjList[node] || []) {[m
[32m+[m[32m      queue.push([...path, neighbor]);[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  return null;[m
[32m+[m[32m}[m
