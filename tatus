[32m+[m
[32m+[m[32m                  nodeColors = {};[m
[32m+[m[32m                  allNodes = nodes.get({ returnType: "Object" });[m
[32m+[m[32m                  for (nodeId in allNodes) {[m
[32m+[m[32m                    nodeColors[nodeId] = allNodes[nodeId].color;[m
[32m+[m[32m                  }[m
[32m+[m[32m                  allEdges = edges.get({ returnType: "Object" });[m
[32m+[m[32m                  // adding nodes and edges to the graph[m
[32m+[m[32m                  data = {nodes: nodes, edges: edges};[m
[32m+[m
[32m+[m[32m                  var options = {"physics": {"enabled": true, "stabilization": {"iterations": 100}, "barnesHut": {"gravitationalConstant": -2000, "springConstant": 0.04, "springLength": 95}}};[m
[32m+[m
[32m+[m[41m                  [m
[32m+[m
[32m+[m
[32m+[m[41m                  [m
[32m+[m
[32m+[m[32m                  network = new vis.Network(container, data, options);[m
[32m+[m
[32m+[m[41m                  [m
[32m+[m
[32m+[m[41m                  [m
[32m+[m
[32m+[m[41m                  [m
[32m+[m
[32m+[m
[32m+[m[41m                  [m
[32m+[m
[32m+[m[32m                  return network;[m
[32m+[m
[32m+[m[32m              }[m
[32m+[m[32m              drawGraph();[m
