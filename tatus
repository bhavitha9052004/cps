[32m+[m
[32m+[m[32m  <script>[m
[32m+[m[41m    [m
[32m+[m[41m    [m
[32m+[m[41m    [m
[32m+[m[41m    [m
[32m+[m[41m    [m
[32m+[m[41m    [m
[32m+[m[32m    'use strict';[m
[32m+[m[32m    const COURSE_DRAWER_LOCAL_STORAGE_KEY = "desktopCourseDrawerState"[m
[32m+[m[32m    const COURSE_DRAWER_OPENED = "opened"[m
[32m+[m[32m    const COURSE_DRAWER_CLOSED = "closed"[m
[32m+[m[41m    [m
[32m+[m[32m    const DESKTOP_COURSE_DRAWER_ID = "desktop-course-drawer"[m
[32m+[m[32m    const COURSE_DRAWER_BTN_ID = "desktop-course-drawer-button"[m
[32m+[m[32m    const MAIN_COURSE_SECTION_ID = "main-course-section"[m
[32m+[m
[32m+[m[32m    try{[m
[32m+[m
[32m+[m[41m      [m
[32m+[m
[32m+[m[32m      function initializeDesktopCourseDrawerState() {[m
[32m+[m[32m        const state = getLocalStorageItem(COURSE_DRAWER_LOCAL_STORAGE_KEY)[m
[32m+[m[32m        if (state === null) {[m
[32m+[m[41m          [m
[32m+[m[32m          setLocalStorageItem(COURSE_DRAWER_LOCAL_STORAGE_KEY, COURSE_DRAWER_OPENED)[m
[32m+[m[32m          showOrHideDesktopCourseDrawer(COURSE_DRAWER_OPENED)[m
[32m+[m[32m        } else {[m
[32m+[m[32m          showOrHideDesktopCourseDrawer(state)[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        document.addEventListener("DOMContentLoaded", () => {[m
[32m+[m[32m          const drawer = $(`#${DESKTOP_COURSE_DRAWER_ID}`)[m
[32m+[m[32m          const mainSection = $(`#${MAIN_COURSE_SECTION_ID}`)[m
[32m+[m
[32m+[m[32m          drawer.on("shown.bs.collapse", (event) => {[m
[32m+[m[32m            if (event.target.id !== DESKTOP_COURSE_DRAWER_ID) {[m
[32m+[m[32m              return;[m
[32m+[m[32m            }[m
[32m+[m[32m            mainSection.addClass("col-lg-9")[m
[32m+[m[32m            mainSection.removeClass("col-12")[m
[32m+[m[32m            setLocalStorageItem(COURSE_DRAWER_LOCAL_STORAGE_KEY, COURSE_DRAWER_OPENED)[m
[32m+[m[32m          })[m
[32m+[m[41m      [m
[32m+[m[32m          drawer.on("hidden.bs.collapse", (event) => {[m
[32m+[m[32m            if (event.target.id !== DESKTOP_COURSE_DRAWER_ID) {[m
[32m+[m[32m              return;[m
[32m+[m[32m            }[m
[32m+[m[32m            mainSection.addClass("col-12")[m
[32m+[m[32m            mainSection.removeClass("col-lg-9")[m
[32m+[m[32m            setLocalStorageItem(COURSE_DRAWER_LOCAL_STORAGE_KEY, COURSE_DRAWER_CLOSED)[m
[32m+[m[32m          })[m
[32m+[m[32m        })[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      function showOrHideDesktopCourseDrawer(state) {[m
[32m+[m[32m        const drawer = document.getElementById(DESKTOP_COURSE_DRAWER_ID)[m
[32m+[m[32m        const button = document.getElementById(COURSE_DRAWER_BTN_ID)[m
[32m+[m[32m        const mainSection = document.getElementById(MAIN_COURSE_SECTION_ID)[m
[32m+[m
[32m+[m[32m        if (state === COURSE_DRAWER_OPENED) {[m
[32m+[m[32m          drawer.classList.add("show")[m
[32m+[m[32m          button.setAttribute("aria-expanded", "true")[m
[32m+[m[32m        } else {[m
[32m+[m[32m          drawer.classList.remove("show")[m
[32m+[m[32m          button.setAttribute("aria-expanded", "false")[m
[32m+[m[32m          mainSection.classList.remove("col-lg-9")[m
[32m+[m[32m          mainSection.classList.add("col-12")[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      function setOrGetLocalStorageItem(action,key,value) {[m
[32m+[m[32m        try {[m
[32m+[m[41m          [m
[32m+[m[32m          if (typeof Storage !== "undefined") {[m
[32m+[m[32m            if (action === "set") {[m
[32m+[m[32m              localStorage.setItem(key, value)[m
[32m+[m[32m              return true[m
[32m+[m[32m            } else {[m
[32m+[m[32m              return localStorage.getItem(key)[m
[32m+[m[32m            }[m
[32m+[m[32m          }[m
[32m+[m[32m          console.error("This browser has no web storage support.")[m
[32m+[m[32m          return null[m
[32m+[m[32m        } catch (e) {[m
[32m+[m[32m          console.error("An exception occurred while storing/fetching data in/from localstorage: ", e)[m
[32m+[m[32m          return null[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      function setLocalStorageItem(key, value) {[m
[32m+[m[32m        return setOrGetLocalStorageItem("set", key, value)[m
[32m+[m[32m      }[m
[32m+[m[41m      [m
[32m+[m[32m      function getLocalStorageItem(key){[m
[32m+[m[32m        return setOrGetLocalStorageItem("get", key)[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      initializeDesktopCourseDrawerState();[m
[32m+[m[32m    }[m
[32m+[m[32m    catch(e){[m
[32m+[m[32m      console.error("Something went wrong in maintaining/toggling course drawer state", e)[m
[32m+[m[32m    }[m
[32m+[m[32m  </script>[m
[32m+[m
[32m+[m[41m  [m
[32m+[m
[32m+[m[32m<script>[m
[32m+[m[32mfunction expandNav(e,t){t.classList.add("show"),e.querySelector(".course-nav-section-toggle, video-tab-toggle-section").setAttribute("aria-expanded","true")}function courseNav(){document.querySelectorAll(".course-nav, .transcript-header").forEach(e=>{e.querySelectorAll(".nav-link").forEach(e=>{const t=e.getAttribute("href")?e.getAttribute("href"):"";if(t.replace(/\/$/,"")===window.location.pathname.replace(/\/$/,"")){e.classList.add("active");const s=e.dataset.uuid,t=e.closest(".course-nav-list-item"),n=t?.querySelector(".collapse"),o=Array.prototype.filter.call(document.querySelectorAll(".course-nav-section-toggle"),e=>e.dataset.uuid===s);t&&n&&o.length>0&&expandNav(t,n)}});const t=e.querySelector("a.nav-link.active");e.querySelectorAll(".course-nav-list-item").forEach(e=>{const n=e.querySelector(".collapse");n&&n.contains(t)&&expandNav(e,n)})})}courseNav()[m
[32m+[m[32m</script>[m
[32m+[m
[32m+[m[41m  [m
[32m+[m
[32m+[m[32m<script>[m
[32m+[m[32m(()=>{var e=()=>document.querySelectorAll("#course-content-section table"),t=()=>document.getElementById("course-content-section");function n(){const o=e(),n=t();if(!n)return;if(!window.ResizeObserver)return;const i=new ResizeObserver(()=>s());i.observe(n),o.forEach(e=>{const t=e.getElementsByTagName("th"),n=e.querySelectorAll("table tr");n.forEach(e=>{const n=e.querySelectorAll("td");n.forEach((e,s)=>{t.length>=s+1&&(n[s].dataset.title=t[s].innerText.trim().concat(": "))})})})}function s(){const s=e(),n=t();if(!n)return;s.forEach(e=>{const t=n.getBoundingClientRect(),o=+(e.dataset.minWidth??0);t.width>o&&e.classList.remove("mobile-table");const s=e.getBoundingClientRect();s.right>t.right&&(e.dataset.minWidth=`${s.width}`,e.classList.add("mobile-table"))})}n()})()[m
