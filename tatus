[36m@@ -0,0 +1,45 @@[m
[32m+[m[32mimport os[m
[32m+[m[32mimport streamlit as st[m
[32m+[m[32mfrom recommendation_engine import load_graph, load_learner_profile, dijkstra_recommendation[m
[32m+[m
[32m+[m[32m# --- File Paths ---[m
[32m+[m[32mBASE_DIR = os.path.dirname(os.path.abspath(__file__))[m
[32m+[m[32mGRAPH_PATH = os.path.join(BASE_DIR, "concept_graph_full.json")[m
[32m+[m[32mPROFILE_PATH = os.path.join(BASE_DIR, "learner_profiles.json")[m
[32m+[m
[32m+[m[32m# --- Load Data ---[m
[32m+[m[32mgraph = load_graph(GRAPH_PATH)[m
[32m+[m
[32m+[m[32m# Get all concept nodes[m
[32m+[m[32mall_nodes = set(graph.keys()) | {n for neighbors in graph.values() for n in neighbors}[m
[32m+[m
[32m+[m[32m# --- UI ---[m
[32m+[m[32mst.set_page_config(page_title="DSA Recommendation Engine", layout="centered")[m
[32m+[m[32mst.title("📘 Personalized Learning Path for DSA")[m
[32m+[m
[32m+[m[32m# Select learner[m
[32m+[m[32mlearner_id = st.text_input("🔎 Enter Learner ID (e.g., L001):", "L001")[m
[32m+[m
[32m+[m[32m# Select target concept[m
[32m+[m[32mtarget_concept = st.selectbox("🎯 Select Target Concept:", sorted(list(all_nodes)))[m
[32m+[m
[32m+[m[32m# Run recommendation on button click[m
[32m+[m[32mif st.button("🚀 Recommend Path"):[m
[32m+[m[32m    try:[m
[32m+[m[32m        learner = load_learner_profile(PROFILE_PATH, learner_id)[m
[32m+[m
[32m+[m[32m        result = dijkstra_recommendation([m
[32m+[m[32m            graph,[m
[32m+[m[32m            start_concepts=learner["mastered_concepts"],[m
[32m+[m[32m            weak_concepts=learner["weak_concepts"],[m
[32m+[m[32m            target=target_concept[m
[32m+[m[32m        )[m
[32m+[m
[32m+[m[32m        if result["path"]:[m
[32m+[m[32m            st.success(f"✅ Recommended Path: {' ➝ '.join(result['path'])}")[m
[32m+[m[32m            st.info(f"💰 Total Cost: {result['cost']}")[m
[32m+[m[32m        else:[m
[32m+[m[32m            st.error("⚠️ No path found to the target concept.")[m
[32m+[m
[32m+[m[32m    except ValueError as e:[m
[32m+[m[32m        st.error(f"❌ {e}")[m
[1mdiff --git a/cg_visualization.py b/cg_visualization.py[m
[1mnew file mode 100644[m
