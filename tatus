[32m+[m[32m        learner_id = f"L{i:03}"[m
[32m+[m[32m        num_mastered = random.randint(4, 8)[m
[32m+[m[32m        num_weak = random.randint(2, 5)[m
[32m+[m
[32m+[m[32m        mastered = set(random.sample(concepts, num_mastered))[m
[32m+[m[32m        weak = set(random.sample([c for c in concepts if c not in mastered], num_weak))[m
[32m+[m
[32m+[m[32m        profile = {[m
[32m+[m[32m            "learner_id": learner_id,[m
[32m+[m[32m            "mastered_concepts": sorted(list(mastered)),[m
[32m+[m[32m            "weak_concepts": sorted(list(weak))[m
[32m+[m[32m        }[m
[32m+[m[32m        profiles.append(profile)[m
[32m+[m[32m    return profiles[m
